        "%%writefile main.cpp\n",
        "\n",
        "#include <iostream>\n",
        "#include <cmath>\n",
        "\n",
        "using namespace std;\n",
        "\n",
        "double f (double x){\n",
        "   return exp(-(x*x));\n",
        "}\n",
        "\n",
        "double derivada(double x, double h){\n",
        "    double derivada = (f(x+h)- f(x-h))/(2*h);\n",
        "    return derivada\n",
        "}\n",
        "\n",
        "int main(){\n",
        "    \n",
        "    return 0\n",
        "}"
      ]
